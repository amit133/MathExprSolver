<?xml version="1.0" encoding="UTF-8"?>
<model>
    <actors>
        <actor>
            <name>electric</name>
            <description>electricity generation company</description>
            <influence>0.2</influence>
            <utility>PE*E - POSUB*OE - PG*G - PRNW*RNW</utility>
        </actor>
        <actor>
            <name>goods</name>
            <description>final goods and services</description>
            <influence>.25</influence>
            <utility>Y - W*N - R*KG - PS*SF</utility>
        </actor>
        <actor>
            <name>govt</name>
            <description>government</description>
            <influence>.2</influence>
            <utility>govUtilW0*welfare - govUtilW1*renewDevi - govUtilW2*oilPriceMove</utility>
        </actor>
        <actor>
            <name>labor</name>
            <description>flower class</description>
            <influence>.2</influence>
            <utility>laborShare*((C^sigmac + d*SH^sigmac)^(1-sigma))/(1-sigma)</utility>
        </actor>
        <actor>
            <name>capital</name>
            <description>upper class</description>
            <influence>.2</influence>
            <utility>(1-laborShare)*((C^sigmac + d*SH^sigmac)^(1-sigma))/(1-sigma)</utility>
        </actor>
    </actors>
    <parameters>
        <parameter>
            <name>alpha</name>
            <value>0.32</value>
        </parameter>
        <parameter>
            <name>beta</name>
            <value>0.42</value>
        </parameter>
        <parameter>
            <name>gamma</name>
            <value>1.0</value>
        </parameter>
        <parameter>
            <name>theta</name>
            <value>0.58221</value>
        </parameter>
        <parameter>
            <name>niu</name>
            <value>-1.381</value>
        </parameter>
        <parameter>
            <name>a</name>
            <value>0.33230</value>
        </parameter>
        <parameter>
            <name>b</name>
            <value>0.00003</value>
        </parameter>
        <parameter>
            <name>d</name>
            <value>0.0028</value>
        </parameter>
        <parameter>
            <name>sigmac</name>
            <value>-0.3333</value>
        </parameter>
        <parameter>
            <name>sigma</name>
            <value>2.0</value>
        </parameter>
        <parameter>
            <name>omega</name>
            <value>0.0735</value>
        </parameter>
        <parameter>
            <name>tau</name>
            <value>1.0</value>
        </parameter>
        <parameter>
            <name>phi</name>
            <value>0.0</value>
        </parameter>
        <parameter>
            <name>betadisc</name>
            <value>0.96</value>
        </parameter>
        <parameter>
            <name>delta</name>
            <value>0.1</value>
        </parameter>
        <parameter>
            <name>nu</name>
            <value>0.05</value>
        </parameter>
        <parameter>
            <name>poaverage</name>
            <value>1.6371</value>
        </parameter>
        <parameter>
            <name>A</name>
            <value>0.0328</value>
        </parameter>
        <parameter>
            <name>rho</name>
            <value>0.9</value>
        </parameter>
        <parameter>
            <name>RBAR</name>
            <value>0.04</value>
        </parameter>
        <parameter>
            <name>bss</name>
            <value>0.16</value>
        </parameter>
        <parameter>
            <name>psi</name>
            <value>0.0</value>
        </parameter>
        <parameter>
            <name>gexg</name>
            <value>0.01398</value>
        </parameter>
        <parameter>
            <name>oexg</name>
            <value>0.4819</value>
        </parameter>
        <parameter>
            <name>posubexg</name>
            <value>0.5</value>
        </parameter>
        <parameter>
            <name>integcost</name>
            <value>0.5</value>
        </parameter>
        <parameter>
            <name>lambda</name>
            <value>-0.25786</value>
        </parameter>
        <parameter>
            <name>targRNW</name>
            <value>9.5</value>
        </parameter>
        <parameter>
            <name>govUtilW0</name>
            <value>0.0</value>
        </parameter>
        <parameter>
            <name>govUtilW1</name>
            <value>0.0</value>
        </parameter>
        <parameter>
            <name>govUtilW2</name>
            <value>0.0</value>
        </parameter>
    </parameters>
    <variables>
        <variable>
            <name>OE0</name>
            <initial>0.0305</initial>
            <compute></compute>
        </variable>
        <variable>
            <name>N0</name>
            <initial>1.0</initial>
            <compute></compute>
        </variable>
        <variable>
            <name>KP0</name>
            <initial>5.6246</initial>
            <compute></compute>
        </variable>
        <variable>
            <name>C0</name>
            <initial>0.9363</initial>
            <compute></compute>
        </variable>
        <variable>
            <name>E0</name>
            <initial></initial>
            <compute>alpha*OE0+beta*gexg+gamma*A*igexg/nu</compute>
        </variable>
        <variable>
            <name>G0</name>
            <initial></initial>
            <compute>gexg</compute>
        </variable>
        <variable>
            <name>RNW0</name>
            <initial></initial>
            <compute>A*igexg/nu</compute>
        </variable>
        <variable>
            <name>PE0</name>
            <initial></initial>
            <compute>1/alpha*posubexg</compute>
        </variable>
        <variable>
            <name>OS0</name>
            <initial></initial>
            <compute>E0*((1-a)/(alpha*a))^(1/(1-lambda))</compute>
        </variable>
        <variable>
            <name>S0</name>
            <initial></initial>
            <compute>(a*E0^lambda+(1-a)*OS0^lambda)^(1/lambda)</compute>
        </variable>
        <variable>
            <name>PS0</name>
            <initial></initial>
            <compute>PE0/a*(E0^(1-lambda))*S0^(lambda-1)</compute>
        </variable>
        <variable>
            <name>SF0</name>
            <initial></initial>
            <compute>KP0*(((RBAR+delta)*b)/(PS0*(1-b)))^(1/(1-niu))</compute>
        </variable>
        <variable>
            <name>W0</name>
            <initial></initial>
            <compute>theta*N0^(theta-1)*((1-b)*KP0^niu+b*SF0^niu)^((1-theta)/niu)</compute>
        </variable>
        <variable>
            <name>R0</name>
            <initial></initial>
            <compute>RBAR+delta</compute>
        </variable>
        <variable>
            <name>SH0</name>
            <initial></initial>
            <compute>S0-SF0</compute>
        </variable>
        <variable>
            <name>RSTAR0</name>
            <initial></initial>
            <compute>RBAR</compute>
        </variable>
        <variable>
            <name>TR0</name>
            <initial></initial>
            <compute>PRNW0*RNW0+POSUB0*(OS0+OE0)+poaverage*(oexg-OS0-OE0)+PG0*gexg-igexg</compute>
        </variable>
        <variable>
            <name>PO0</name>
            <initial></initial>
            <compute>poaverage</compute>
        </variable>
        <variable>
            <name>B0</name>
            <initial></initial>
            <compute>-bss/RBAR*N0^theta*((1-b)*KP0^niu+b*SF0^niu)^((1-theta)/niu)</compute>
        </variable>
        <variable>
            <name>O0</name>
            <initial></initial>
            <compute>oexg</compute>
        </variable>
        <variable>
            <name>KG0</name>
            <initial></initial>
            <compute>igexg/nu</compute>
        </variable>
        <variable>
            <name>Y0</name>
            <initial></initial>
            <compute>N0^theta*((1-b)*KP0^niu+b*SF0^niu)^((1-theta)/niu)</compute>
        </variable>
    </variables>
    <policy>
        <variable>
            <name>POSUB0</name>
            <base>0.5</base>
        </variable>
        <variable>
            <name>PG0</name>
            <base>0.6563</base>
        </variable>
        <variable>
            <name>PRNW0</name>
            <base>1.5625</base>
        </variable>
        <constant>
            <name>igexg</name>
            <base>0</base>
        </constant>
        <constant>
            <name>laborShare</name>
            <base>0.5</base>
        </constant>
    </policy>
    <optimize>
        <equation>
            <name>OE</name>
            <function>E-alpha*OE-beta*G-gamma*RNW</function>
        </equation>
        <equation>
            <name>E</name>
            <function>PE-1/alpha*POSUB</function>
        </equation>
        <equation>
            <name>G</name>
            <function>PG-beta/alpha*POSUB</function>
        </equation>
        <equation>
            <name>RNW</name>
            <function>PRNW-gamma/alpha*POSUB</function>
        </equation>
        <equation>
            <name>PE</name>
            <function>S-(a*E^lambda+(1-a)*OS^lambda)^(1/lambda)</function>
        </equation>
        <equation>
            <name>POSUB</name>
            <function>PS-POSUB*(OS^(1-lambda))/(1-a)*(a*E^lambda+(1-a)*OS^lambda)^((lambda-1)/lambda)</function>
        </equation>
        <equation>
            <name>PG</name>
            <function>PS-PE*(E^(1-lambda))/a*(a*E^lambda+(1-a)*OS^lambda)^((lambda-1)/lambda)</function>
        </equation>
        <equation>
            <name>PRNW</name>
            <function>W-theta*N^(theta-1)*((1-b)*KP^niu+b*SF^niu)^((1-theta)/niu)</function>
        </equation>
        <equation>
            <name>S</name>
            <function>R-(1-theta)*(1-b)*KP^(niu-1)*N^theta*((1-b)*KP^niu+b*SF^niu)^((1-theta)/niu-1)</function>
        </equation>
        <equation>
            <name>OS</name>
            <function>PS-(1-theta)*b*SF^(niu-1)*N^theta*((1-b)*KP^niu+b*SF^niu)^((1-theta)/niu-1)</function>
        </equation>
        <equation>
            <name>PS</name>
            <function>N-1</function>
        </equation>
        <equation>
            <name>N</name>
            <function>PS-d*(SH/C)^(sigmac-1)</function>
        </equation>
        <equation>
            <name>KP</name>
            <function>(B-(1+RSTAR)*B)-bss*(Y+PO*(O-OE-OS))</function>
        </equation>
        <equation>
            <name>SF</name>
            <function>1-betadisc*(1-delta+R)</function>
        </equation>
        <equation>
            <name>W</name>
            <function>B+KP-(1-delta)*KP+C+PS*SH-W*N-R*KP-(1+RSTAR)*B-TR</function>
        </equation>
        <equation>
            <name>R</name>
            <function>PRNW*RNW+POSUB*(OS+OE)+PO*(O-OS-OE)+PG*G-TR-igexg</function>
        </equation>
        <equation>
            <name>SH</name>
            <function>RNW-A/(1+integcost*RNW/E)*KG</function>
        </equation>
        <equation>
            <name>C</name>
            <function>S-SF-SH</function>
        </equation>
        <equation>
            <name>RSTAR</name>
            <function>PO-poaverage</function>
        </equation>
        <equation>
            <name>TR</name>
            <function>RSTAR-RBAR</function>
        </equation>
        <equation>
            <name>PO</name>
            <function>nu*KG-igexg</function>
        </equation>
        <equation>
            <name>B</name>
            <function>POSUB-posubexg</function>
        </equation>
        <equation>
            <name>O</name>
            <function>O-oexg</function>
        </equation>
        <equation>
            <name>KG</name>
            <function>G-gexg</function>
        </equation>
        <equation>
            <name>Y</name>
            <function>Y-N^theta*((1-b)*KP^niu+b*SF^niu)^((1-theta)/niu)</function>
        </equation>
    </optimize>
    <equations>
        <equation>
            <name>GDP</name>
            <function>Y+PO*(O-OE-OS)</function>
        </equation>
        <equation>
            <name>welfare</name>
            <function>1/(1-sigma)*(C^sigmac+d*SH^sigmac)^((1-sigma)/sigmac)</function>
        </equation>
        <equation>
            <name>renewDevi</name>
            <function>abs(RNW-targRNW)</function>
        </equation>
        <equation>
            <name>oilPriceMove</name>
            <function>poaverage/PO</function>
        </equation>
    </equations>
</model>